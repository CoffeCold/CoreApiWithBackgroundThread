    <title>Details</title>
</head>
<body>

    <div>
        <h4>Job</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.JobId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.JobId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Description)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Description)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ExecutionDomain)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ExecutionDomain)
            </dd>
            <dt class="col-sm-2">
